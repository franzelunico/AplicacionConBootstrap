    "url": "https://github.com/franzelunico/cursoBootstrap/issues"
  },
  "homepage": "https://github.com/franzelunico/cursoBootstrap#readme",
  "devDependencies": {
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^4.1.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "bootstrap": "^4.1.2",
    "concurrently": "^4.0.1",
    "jquery": "^1.9.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  }
